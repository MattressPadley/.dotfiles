{
    "git.openRepositoryInParentFolders": "never",
    "git.ignoreRebaseWarning": true,
    "prettier.singleQuote": true,
    "prettier.singleAttributePerLine": true,
    "editor.formatOnPaste": true,
    "editor.formatOnType": true,
    "editor.fontFamily": "UbuntuSansMono Nerd Font",
    "editor.fontSize": 10,
    "editor.fontLigatures": true,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.autohide": false,
    "editor.inlineSuggest.enabled": true,
    "editor.smoothScrolling": true,
    "editor.lightbulb.enabled": "off",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorBlinking": "solid",
    "editor.cursorStyle": "line",
    "editor.lineHeight": 1.6,
    "editor.scrollbar.vertical": "hidden",
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.verticalScrollbarSize": 0,
    "editor.hideCursorInOverviewRuler": false,
    "editor.overviewRulerBorder": false,
    "workbench.iconTheme": "catppuccin-mocha",
    "workbench.colorTheme": "Catppuccin Mocha",
    "workbench.sideBar.location": "right",
    "workbench.list.smoothScrolling": true,
    "workbench.editor.scrollToSwitchTabs": true,
    "workbench.startupEditor": "none",
    "workbench.statusBar.visible": false,
    "workbench.editor.showTabs": "single",
    "workbench.tips.enabled": false,
    "workbench.activityBar.location": "hidden",
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 10000,
    "breadcrumbs.enabled": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.compactFolders": false,
    "terminal.integrated.smoothScrolling": true,
    "terminal.integrated.fontFamily": "UbuntuSansMono Nerd Font",
    "terminal.integrated.tabs.enabled": false,
    "window.titleBarStyle": "native",
    "window.customTitleBarVisibility": "never",

    "custom-ui-style.electron": {
        "frame": false,
        "titleBarStyle": "customButtonsOnHover"
    },
    "custom-ui-style.stylesheet": {
        ".monaco-workbench .part > .title > .title-label": "display: none",
        ".title-actions": "display: none !important",
        ".quick-input-widget": {
            "box-shadow": "0 5px 10px rgba(0,0,0,0), 0 0 0 100vw rgba(0,0,0,0.15) !important",
            "top": "25vh !important",
          },
          ".quick-input-widget .quick-input-list .quick-input-list-entry.quick-input-list-separator-border": {
            "border-top-width": "0px !important"
          },
        },


    "custom-ui-style.reloadWithoutPrompting": true,
