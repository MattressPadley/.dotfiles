    // Vim bindings
    "vim.leader": "<Space>",
    "vim.normalModeKeyBindings": [
        // Navigation
        {"before": ["w"], "after": ["k"]},
        {"before": ["s"], "after": ["j"]},
        {"before": ["a"], "after": ["h"]},
        {"before": ["d"], "after": ["l"]},

        // splits
        {"before": ["leader", "v"], "commands": [":vsplit "]},
        {"before": ["leader", "h"], "commands": [":split "]},
   
        //Pane Navigation
        {"before": ["leader", "w"], "commands": ["workbench.action.focusAboveGroup"]},
        {"before": ["leader", "s"], "commands": ["workbench.action.focusBelowGroup"]},
        {"before": ["leader", "a"], "commands": ["workbench.action.focusLeftGroup"]},
        {"before": ["leader", "d"], "commands": ["workbench.action.focusRightGroup"]}

    ],

    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "extensions.ignoreRecommendations": true,
    "[xml]": {
        "editor.defaultFormatter": "redhat.vscode-xml"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "redhat.telemetry.enabled": true,
    "[code-text-binary]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "diffEditor.maxComputationTime": 0,
    "animations.Install-Method": "Custom UI Style",
    "cursor.cpp.enablePartialAccepts": true,
    "cursor.cmdk.useThemedDiffBackground": true,
    "cursor.chat.showSuggestedFiles": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
